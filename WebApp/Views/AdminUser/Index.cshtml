@model IEnumerable<AppIdentityUser>

@{
	ViewData["Title"] = "All Users";
}

<partial name="~/Views/Partials/_Breadcrumb.cshtml" />
<div class="container mt-5">
	@foreach (var user in Model)
	{
			<div class="d-flex justify-content-between align-items-center">
				<div class="mb-3 d-flex align-items-center">
				<img src="/images/placeholders/@user.ProfileImageUrl" alt="@user.FirstName" />
					<div class="ms-2">
						<div class="display-6"><strong>@user.FirstName-@user.LastName</strong></div>
						<div>@user.Email</div>
						<div>@user.PhoneNumber</div>

					</div>
					<div class="ms-2">
						<div>@user.Addresses.ToList()</div>
					</div>
				</div>
				
			</div>
	}
</div>
